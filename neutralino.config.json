{
  "applicationId": "com.filetransfer.sparks",
  "version": "0.0.3",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources/",
  "url": "/index.html",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "nativeAllowList": [
    "app.*",
    "os.*",
    "filesystem.*",
    "events.*",
    "storage.*",
    "window.*"
  ],
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "modes": {
    "window": {
      "title": "File Transfer for DGX Sparks",
      "width": 1024,
      "height": 1088,
      "minWidth": 1000,
      "minHeight": 650,
      "resizable": true,
      "maximize": false,
      "enableInspector": false,
      "openInspectorOnStartup": false
    }
  },
  "cli": {
    "binaryName": "file-transfer-sparks",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "6.4.0",
    "clientVersion": "6.5.0"
  }
}
